{
  "name": "@snowlynxsoftware/ozs-server",
  "version": "2024.1.1",
  "scripts": {
    "start": "npm run serve:local",
    "build": "rm -rf ./dist && npx tsc",
    "serve:local": "npx tsc-watch --onSuccess \"node --env-file ./.env ./dist/main.js\"",
    "serve:production": "node --env-file ./.env ./dist/main.js"
  },
  "dependencies": {
    "eta": "^3.2.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  }
}
